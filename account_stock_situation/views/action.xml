<?xml version="1.0" encoding="UTF-8" ?>
<odoo noupdate="1">

    <record id="stock_account_valuation_main_company" model="ir.cron">
        <field name="name">Stock Situation</field>
        <field name="model_id" ref="base.model_res_company" />
        <field name="state">code</field>
        <field
            name="code"
        >model._set_account_stock_valuation('base.main_company')</field>
        <field name="interval_number">1</field>
        <field name="interval_type">months</field>
        <field name="numbercall">-1</field>
        <field
            name="nextcall"
            eval="(datetime.now()+relativedelta(months=1)).strftime('%Y-%m-01 01:30:00')"
        />
        <field name="active">False</field>
    </record>

</odoo>
