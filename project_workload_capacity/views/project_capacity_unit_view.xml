<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="project_capacity_unit_view_tree" model="ir.ui.view">
        <field name="model">project.capacity.unit</field>
        <field name="arch" type="xml">
            <tree string="Capacity Unit">
                <field name="capacity_id" />
                <field name="week" />
                <field name="hours" />
            </tree>
        </field>
    </record>

    <record id="project_capacity_unit_view_search" model="ir.ui.view">
        <field name="model">project.capacity.unit</field>
        <field name="arch" type="xml">
            <search string="Capacity Unit">
                <field name="capacity_id" />
                <field name="week" />
            </search>
        </field>
    </record>

    <record model="ir.actions.act_window" id="project_capacity_unit_action">
        <field name="name">Capacity Unit</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">project.capacity.unit</field>
        <field name="view_mode">tree</field>
        <field name="search_view_id" ref="project_capacity_unit_view_search" />
        <field name="domain">[]</field>
        <field name="context">{}</field>
    </record>

</odoo>
